{
  "name": "@vishalsangave/sample-cli",
  "description": "Sample code to test working of oclif",
  "version": "1.1.0-beta.1",
  "author": "Vishal Sangave @vishalsangave",
  "bin": {
    "sample": "./bin/run"
  },
  "bugs": "https://github.com/vishalsangave/sample-cli/issues",
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.10",
    "@oclif/test": "^1.2.9",
    "@semantic-release/npm": "^9.0.1",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.60",
    "chai": "^4.3.6",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "semantic-release": "^19.0.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/changelog": "^6.0.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/vishalsangave/sample-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "sample"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]"
        }
      ]
    ]
  },
  "repository": "vishalsangave/sample-cli",
  "scripts": {
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@oclif/command": "^1.8.16",
    "@oclif/config": "^1.18.3",
    "@oclif/plugin-help": "^3.3.1",
    "tslib": "^1.14.1"
  }
}
